<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="GPSDialogFragment" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:textColor">#EAE2E5</item>
        <item name="android:textColorPrimary">#EDECEC</item>
        <item name="android:background">#430D1F</item>
        <item name="android:textAppearanceMedium">@style/TextAppearance.AppCompat.Medium</item>
    </style>

    <style name="LocationPermissionDialogFragment" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:textColor">#EAE2E5</item>
        <item name="android:textColorPrimary">#EDECEC</item>
        <item name="android:background">#430D1F</item>
        <item name="android:textAppearanceMedium">@style/TextAppearance.AppCompat.Medium</item>
    </style>

</resources>
